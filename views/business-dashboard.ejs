                      <ul class="pagination custom-pagination">
                        <% maxVisiblePages = 9 %>
                        <% if (totalPages <= maxVisiblePages) { %>
                        <!-- Show all pages if total pages are less than or equal to maxVisiblePages -->
                        <% for (let i = 1; i <= totalPages; i++) { %>
                        <% if (i === currentPage) { %>
                        <li><span class="active"><%= i %></span></li>
                        <% } else { %>
                        <li><a href="?page=<%= i %>"><%= i %></a></li>
                        <% } %>
                        <% } %>
                        <% } else { %>
                        <!-- Show pagination with ellipses if total pages are more than maxVisiblePages -->
                        <% if (currentPage > 1) { %>
                        <li><a href="?page=<%= currentPage - 1 %>">&laquo;</a></li>
                        <% } %>

                        <% if (currentPage > 3) { %>
                        <li><a href="?page=1">1</a></li>
                        <% if (currentPage > 4) { %>
                        <li><span>...</span></li>
                        <% } %>
                        <% } %>

                        <!-- Display current page and nearby pages -->
                        <% for (let i = Math.max(1, currentPage - 2); i <= Math.min(totalPages, currentPage + 2); i++) { %>
                        <% if (i === currentPage) { %>
                        <li><span class="active"><%= i %></span></li>
                        <% } else { %>
                        <li><a href="?page=<%= i %>"><%= i %></a></li>
                        <% } %>
                        <% } %>

                        <% if (currentPage < totalPages - 2) { %>
                        <% if (currentPage < totalPages - 3) { %>
                        <li><span>...</span></li>
                        <% } %>
                        <li><a href="?page=<%= totalPages %>"><%= totalPages %></a></li>
                        <% } %>

                        <% if (currentPage < totalPages) { %>
                        <li><a href="?page=<%= currentPage + 1 %>">&raquo;</a></li>
                        <% } %>
                        <% } %>
